{
  "name": "@gitbox/comment",
  "version": "0.0.0",
  "description": "a comment library base on github issues",
  "author": "lencx <cxin1314@gmail.com>",
  "homepage": "https://github.com/lencx/gitbox",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "keywords": [
    "github",
    "gitbox",
    "issues",
    "comment"
  ],
  "directories": {
    "dist": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.yarnpkg.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lencx/gitbox.git"
  },
  "scripts": {
    "watch": "tsc --outDir dist/cjs",
    "build:cjs": "tsc --outDir dist/cjs",
    "build:esm": "tsc --module es2015 --target ESNext --outDir dist/esm",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "bugs": {
    "url": "https://github.com/lencx/gitbox/issues"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.1"
  },
  "dependencies": {
    "@gitbox/utils": "^0.0.0"
  }
}
