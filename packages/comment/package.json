{
  "name": "@gitbox/comment",
  "version": "0.0.0",
  "description": "a comment library base on github issues",
  "author": "lencx <cxin1314@gmail.com>",
  "homepage": "https://github.com/lencx/gitbox",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "watch": "rimraf lib && tsc --watch",
    "build:cjs": "tsc",
    "build:esm": "tsc --module es2015 --outDir esm",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "keywords": [
    "github",
    "gitbox",
    "issues",
    "comment"
  ],
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "esm"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lencx/gitbox.git"
  },
  "bugs": {
    "url": "https://github.com/lencx/gitbox/issues"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.0.1"
  },
  "dependencies": {
    "@gitbox/utils": "file:../utils",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "react-is": ">= 16.8.0",
    "styled-components": "^5.0.1"
  }
}
